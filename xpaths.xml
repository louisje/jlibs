    <testcase>
        <file>xmlFiles/web.xml</file>

        <xpath>/web-app/servlet/servlet-name</xpath>
        <xpath>/web-app/servlet[1]/servlet-name</xpath>
        <xpath>/web-app/servlet[1]/servlet-name/text()</xpath>
